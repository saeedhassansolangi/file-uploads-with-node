<%-include("partials/headers")%>

<div class="row">
  <%profile.forEach(profile =>{%>
  <div class="col-md-3 m-4">
    <div style="width: 18rem;">
      <img src=" <%=profile.image%>" class="card-img-top" style="width: 300px;" alt="">
      <div class="card-body">
        <div class="card-title">
          <h4><%=profile.name%></h4>
          <div class="card-text">
            <p><%=profile.bio%></p>
          </div>
        </div>
      </div>
    </div>
  </div>
  <%})%>
</div>
</div>